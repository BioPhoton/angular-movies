{
  "name": "_movies-user-flows",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "movies-user-flows/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/movies-user-flows",
        "main": "movies-user-flows/src/index.ts",
        "tsConfig": "movies-user-flows/tsconfig.lib.json",
        "assets": [
          "movies-user-flows/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "movies-user-flows/**/*.ts"
        ]
      }
    },
    "user-flow": {
      "executor": "@push-based/user-flow-nx-plugin:user-flow",
      "options": {
        "rcPath": "projects/movies-user-flows/.user-flowrc.json",
        "outputPath": "dist/user-flows/movies-user-flows"
      },
      "configurations": {
        "prerender": {
          "url": "http://localhost:4201",
          "outputPath": "dist/user-flows/movies-user-flows",
          "serveCommand": "nx run ng-universal-express:prerender:production",
          "awaitServeStdout": "Angular Live Development Server is listening on"
        },
        "ng-universal-ssr": {
          "url": "http://localhost:4201",
          "outputPath": "dist/user-flows/movies-user-flows",
          "serveCommand": "nx run movies:serve-ssr:production",
          "awaitServeStdout": "Angular Live Development Server is listening on"
        }
      }
    }
  },
  "tags": []
}
