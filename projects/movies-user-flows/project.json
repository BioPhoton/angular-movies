{
  "name": "movies-user-flows",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "movies-user-flows/src",
  "projectType": "library",
  "targets": {
    "build-lib": {
      "dependsOn": [],
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/projects/movies-user-flows",
        "main": "movies-user-flows/src/index.ts",
        "tsConfig": "movies-user-flows/tsconfig.lib.json",
        "assets": []
      }
    },
    "_user-flow": {
      "executor": "@push-based/user-flow-nx-plugin:user-flow",
      "options": {
        "rcPath": "projects/movies-user-flows/.user-flowrc.json",
        "outputPath": "dist/user-flow/movies-user-flows"
      },
      "configurations": {
        "development": {
          "verbose": true,
          "dryRun": true,
          "url": "http://localhost:4201",
          "outputPath": "dist/user-flows/movies-user-flows",
          "serveCommand": "nx run firebase-function:serve:production",
          "awaitServeStdout": "Angular Live Development Server is listening on"
        },
        "production": {
          "dryRun": false,
          "url": "http://localhost:4201",
          "outputPath": "dist/user-flows/movies-user-flows",
          "serveCommand": "nx run ng-universal-express:serve:production",
          "awaitServeStdout": "Angular Live Development Server is listening on"
        },
        "prerender": {
          "url": "http://localhost:4201",
          "outputPath": "dist/user-flows/movies-user-flows",
          "serveCommand": "nx run ng-universal-express:serve:production",
          "awaitServeStdout": "Angular Live Development Server is listening on"
        },
        "prerender+ng-universal-ssr": {
          "url": "http://localhost:4301",
          "outputPath": "dist/user-flows/movies-user-flows",
          "serveCommand": "nx run ng-universal-express:serve:production",
          "awaitServeStdout": "Angular Live Development Server is listening on"
        },
        "prerender+ng-universal-ssr+firebase-fn": {
          "url": "http://localhost:4201",
          "outputPath": "dist/user-flows/movies-user-flows",
          "serveCommand": "nx run firebase-function:serve",
          "awaitServeStdout": "Angular Live Development Server is listening on"
        }
      },
      "defaultConfiguration": "production"
    }
  },
  "tags": []
}
