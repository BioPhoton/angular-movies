{
  "collect": {
    "url": "http://localhost:4200",
    "ufPath": "./projects/movies-user-flows/src/user-flows/ci-flow.uf.ts",
    "serveCommand": "npm run start",
    "awaitServeStdout": "Angular Live Development",
    "config": {
      "settings": {
        "formFactor": "desktop",
        "screenEmulation": {
          "mobile": false,
          "width": 1350,
          "height": 940,
          "deviceScaleFactor": 1,
          "disabled": false
        }
      }
    }
  },
  "persist": {
    "outPath": "./projects/movies-user-flows/src/measures",
    "format": [
      "md"
    ]
  }
}
